package shocky3.ident;

import org.pircbotx.User;
import shocky3.BotManager;

public class NickIdentHandler extends IdentHandler {
	public NickIdentHandler() {
		super("n");
	}
	
	public IdentHandler copy() {
		return new NickIdentHandler();
	}
	
	public boolean checkAvailability() {
		return true;
	}
	
	public String account(BotManager bm, User user) {
		return user.getNick();
	}
	
	public boolean isAccount(BotManager bm, User user, String account) {
		String acc = account(bm, user);
		return acc.equals(account);
	}
}