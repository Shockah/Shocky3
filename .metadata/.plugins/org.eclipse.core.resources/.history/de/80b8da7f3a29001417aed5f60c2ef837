<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="build" name="SConsole">
	<target name="build">
		<mkdir dir="bin"/>
		<delete file="../Shocky3/plugins/sconsole.jar"/>
		<jar destfile="../Shocky3/plugins/sconsole.jar" filesetmanifest="mergewithoutmain">
		<manifest>
			<attribute name="Class-Path" value=". libs/lanterna-2.1.7.jar"/>
		</manifest>
		<fileset dir="bin"/>
		</jar>
	</target>
</project>