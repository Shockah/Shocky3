package scommands;

import org.pircbotx.PircBotX;
import org.pircbotx.hooks.events.MessageEvent;

public class CommandReverse extends Command {
	public CommandReverse() {
		super("reverse");
	}
	
	public void call(MessageEvent<PircBotX> e, String trigger, String args) {
		e.respond(new StringBuilder(args).reverse().toString());
	}
}